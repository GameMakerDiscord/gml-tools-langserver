{
	"$schema": "http://json-schema.org/draft-06/schema#",
	"$ref": "#/definitions/GMLDocSchema",
	"definitions": {
		"GMLDocSchema": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"functions": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/Function"
					}
				},
				"objectsAndInstanceVariables": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/ObjectsAndInstanceVariable"
					}
				}
			},
			"required": ["functions", "objectsAndInstanceVariables"],
			"title": "GMLDocSchema"
		},
		"Function": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"name": {
					"type": "string"
				},
				"signature": {
					"type": "string"
				},
				"parameters": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/Parameter"
					}
				},
				"example": {
					"$ref": "#/definitions/Example"
				},
				"documentation": {
					"type": "string"
				},
				"return": {
					"type": "string"
				}
			},
			"required": ["documentation", "example", "name", "parameters", "return", "signature"],
			"title": "Function"
		},
		"Example": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"code": {
					"type": "string"
				},
				"description": {
					"type": "string"
				}
			},
			"required": ["code", "description"],
			"title": "Example"
		},
		"Parameter": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"label": {
					"type": "string"
				},
				"documentation": {
					"type": "string"
				},
				"required": {
					"type": "boolean"
				}
			},
			"required": ["documentation", "label", "required"],
			"title": "Parameter"
		},
		"ObjectsAndInstanceVariable": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"name": {
					"type": "string"
				},
				"description": {
					"type": "string"
				},
				"instanceVariables": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/InstanceVariable"
					}
				}
			},
			"required": ["description", "instanceVariables", "name"],
			"title": "ObjectsAndInstanceVariable"
		},
		"InstanceVariable": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"label": {
					"type": "string"
				},
				"description": {
					"type": "string"
				}
			},
			"required": ["description", "label"],
			"title": "InstanceVariable"
		}
	}
}
